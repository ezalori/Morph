{
  "name": "morph-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel serve mock",
    "start:proxy": "npm run serve",
    "mock": "nodemon --watch mock mock/index.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "element-ui": "^2.4.5",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "qs": "^6.5.2",
    "vue": "^2.5.16",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "@types/qs": "^6.5.1",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.0.1",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.16"
  }
}
